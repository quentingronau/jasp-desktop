{
	"name": "BinomialTest",
	"autorun": true,
	"version": "1.00",
	"options": [
	  {
	    "name": "variables",
	    "type": "Variables"
	  },
		{
			"format": "",
	   	"name": "testValue",
			"type": "Number",
	  	"value": 0.5,
	  	"max": 1,
	  	"min": 0
		},
	  {
	  	"name": "hypothesis",
	    "options": [ "notEqualToTestValue", "greaterThanTestValue", "lessThanTestValue" ],
	    "default": "notEqualToTestValue",
	    "type": "List"
	  },
		{
			"name": "confidenceInterval",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "confidenceIntervalInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "descriptivesPlots",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "descriptivesPlotsConfidenceInterval",
			"type": "Number",
			"value": 0.950
		},
		{
		  "name": "VovkSellkeMPR",
		  "type": "Boolean",
		  "default": false
		},
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 160
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 300
		}
	]
}
