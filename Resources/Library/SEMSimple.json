{
	"name": "SEMSimple",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "model",
			"type": "String"
		},
		{
			"name": "errorCalculation",
			"type": "List",
			"options": [
				"standard",
				"robust",
				"bootstrap"
			],
			"default": "standard"
		},
		{
			"name": "errorCalculationBootstrapSamples",
			"type": "Integer",
			"default": 1000
		},
		{
			"name": "outputMardiasCoefficients",
			"type": "Boolean"
		},
		{
			"name": "outputAdditionalFitMeasures",
			"type": "Boolean"
		},
		{
			"name": "outputFittedCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "outputResidualCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "outputObservedCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "outputModificationIndices",
			"type": "Boolean"
		},
		{
			"name": "outputModificationIndicesHideLowIndices",
			"type": "Boolean"
		},
		{
			"name": "outputModificationIndicesHideLowIndicesThreshold",
			"type": "Integer",
			"default": 10
		},
		{
			"name": "outputObservedCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "groupingVariable",
			"type": "Variable"
		},
		{
			"name": "estimator",
			"type": "List",
			"options": [
				"automatic",
				"ML",
				"GLS",
				"WLS",
				"ULS",
				"DWLS"
			],
			"default": "automatic"
		},
		{
			"name": "includeMeanStructure",
			"type": "Boolean"
		},
		{
			"name": "assumeFactorsUncorrelated",
			"type": "Boolean"
		},
		{
			"name": "fixExogenousCovariates",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "factorStandardisation",
			"type": "List",
			"options": [
				"residualVariance",
				"factorLoadings",
				"none"
			]
		},
		{
			"name": "fixManifestInterceptsToZero",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "fixLatentInterceptsToZero",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "omitResidualSingleIndicator",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "residualVariances",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "correlateExogenousLatents",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "addThresholds",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "addScalingParameters",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "correlateDependentVariables",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "emulation",
			"type": "List",
			"options": [
				"none",
				"Mplus",
				"EQS"
			],
			"default": "none"
		}
	]
}
