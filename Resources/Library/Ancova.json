{
	"name": "Ancova",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "fixedFactors",
			"type": "Variables"
		},
		{
			"name": "randomFactors",
			"type": "Variables"
		},
		{
			"name": "covariates",
			"type": "Variables"
		},
		{
			"name": "wlsWeights",
			"type": "Variable"
		},
		{
			"name": "modelTerms",
			"template": [
				{
					"name": "components",
					"type": "Term"
				}
			],
			"type": "Table"
		},
		{
			"name": "sumOfSquares",
			"options": [ "type1", "type2", "type3" ],
			"default": "type3",
			"type": "List"
		},
		{
			"name": "contrasts",
			"template": [
				{
					"name": "variable",
					"type": "Variable"
				},
				{
					"name": "contrast",
					"options": [
						"none",
						"deviation",
						"simple",
						"difference",
						"Helmert",
						"repeated",
						"polynomial"
					],
					"default": "none",
					"type": "List"
				}
			],
			"type": "Table"
		},
		{
			"name": "postHocTestsVariables",
			"type": "Variables"
		},
		{
			"name": "postHocTestsBonferroni",
			"type": "Boolean"
		},
		{
			"name": "postHocTestsHolm",
			"type": "Boolean"
		},
		{
			"name": "postHocTestsTukey",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "postHocTestsScheffe",
			"type": "Boolean"
		},
		{
			"name": "marginalMeansTerms",
			"type": "Terms"
		},
		{
			"name": "marginalMeansCompareMainEffects",
			"type": "Boolean"
		},
		{
			"name": "marginalMeansCIAdjustment",
			"options": [ "none", "bonferroni", "sidak" ],
			"default": "none",
			"type": "List"
		},
		{
			"name": "descriptives",
			"type": "Boolean"
		},
		{
			"name": "effectSizeEstimates",
			"type": "Boolean"
		},
		{
			"name": "homogeneityTests",
			"type": "Boolean"
		},
		{
			"name": "factorCovariateIndependence",
			"type": "Boolean"
		},
		{
			"name": "plotHorizontalAxis",
			"type": "Variable"
		},
		{
			"name": "plotSeparateLines",
			"type": "Variable"
		},
		{
			"name": "plotSeparatePlots",
			"type": "Variable"
		},
		{
			"name": "plotErrorBars",
			"type": "Boolean"
		},
		{
			"name": "plotWidthDescriptivesPlotLegend",
			"type": "Integer",
			"default": 450
		},
		{
			"name": "plotHeightDescriptivesPlotLegend",
			"type": "Integer",
			"default": 300
		},
		{
			"name": "plotWidthDescriptivesPlotNoLegend",
			"type": "Integer",
			"default": 350
		},
		{
			"name": "plotHeightDescriptivesPlotNoLegend",
			"type": "Integer",
			"default": 300
		},
		{
			"name": "effectSizeEtaSquared",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "effectSizePartialEtaSquared",
			"type": "Boolean"
		},
		{
			"name": "effectSizeOmegaSquared",
			"type": "Boolean"
		},
		{
			"name": "errorBarType",
			"options": [ "confidenceInterval", "standardError" ],
			"default": "confidenceInterval",
			"type": "List"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.000,
			"name": "confidenceIntervalInterval",
			"type": "Number",
			"value": 0.95
		},
		{
		  "name": "VovkSellkeMPR",
		  "type": "Boolean",
		  "default": false
		},
		{
			"name": "qqPlot",
			"type": "Boolean"
		},
		{
			"name": "plotWidthQQPlot",
			"type": "Integer",
			"default": 300
		},
		{
			"name": "plotHeightQQPlot",
			"type": "Integer",
			"default": 300
		}
	]
}
