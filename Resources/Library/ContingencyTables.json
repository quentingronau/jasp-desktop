{
	"name": "ContingencyTables",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "rows",
			"type": "Variables"
		},
		{
			"name": "columns",
			"type": "Variables"
		},
		{
			"name": "counts",
			"type": "Variable"
		},
		{
			"name": "layers",
			"type": "Table",
			"template": [
				{
					"default": "Layer %1",
					"name": "name",
					"type": "String"
				},
				{
					"name": "variables",
					"type": "Variables"
				}
			]
		},
		{
			"name": "chiSquared",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "chiSquaredContinuityCorrection",
			"type": "Boolean"
		},
		{
			"name": "likelihoodRatio",
			"type": "Boolean"
		},
		{
			"name": "oddsRatio",
			"type": "Boolean"
		},
		{
			"name": "oddsRatioConfidenceIntervalInterval",
			"type": "Number",
			"format": "%",
			"max": 1.0,
			"min": 0.0,
			"value": 0.950
		},
		{
			"name": "contingencyCoefficient",
			"type": "Boolean"
		},
		{
			"name": "phiAndCramersV",
			"type": "Boolean"
		},
		{
			"name": "lambda",
			"type": "Boolean"
		},
		{
			"name": "uncertaintyCoefficient",
			"type": "Boolean"
		},
		{
			"name": "gamma",
			"type": "Boolean"
		},
		{
			"name": "somersD",
			"type": "Boolean"
		},
		{
			"name": "kendallsTauB",
			"type": "Boolean"
		},
		{
			"name": "kendallsTauC",
			"type": "Boolean"
		},
		{
			"name": "byIntervalEta",
			"type": "Boolean"
		},
		{
			"name": "cochransAndMantel",
			"type": "Boolean"
		},
		{
			"name": "testOddsRatioEquals",
			"type": "Number",
			"value": 1.0
		},
		{
			"name": "countsObserved",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "countsExpected",
			"type": "Boolean"
		},
		{
			"name": "hideSmallCounts",
			"type": "Boolean"
		},
		{
			"name": "hideSmallCountsLessThan",
			"type": "Integer",
			"default": 5
		},
		{
			"name": "zTestCompareColumns",
			"type": "Boolean"
		},
		{
			"name": "zTestAdjustPValues",
			"type": "Boolean"
		},
		{
			"name": "percentagesRow",
			"type": "Boolean"
		},
		{
			"name": "percentagesColumn",
			"type": "Boolean"
		},
		{
			"name": "percentagesTotal",
			"type": "Boolean"
		},
		{
			"name": "residualsUnstandardized",
			"type": "Boolean"
		},
		{
			"name": "residualsStandardized",
			"type": "Boolean"
		},
		{
			"name": "residualsAdjustedStandardized",
			"type": "Boolean"
		},
		{
		  "name": "VovkSellkeMPR",
		  "type": "Boolean",
		  "default": false
		},
		{
			"name": "rowOrder",
			"type": "List",
			"options": [ "ascending", "descending" ],
			"default": "ascending"
		},
		{
			"name": "columnOrder",
			"type": "List",
			"options": [ "ascending", "descending" ],
			"default": "ascending"
		}
	]
}
