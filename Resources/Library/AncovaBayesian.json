{
	"name": "AncovaBayesian",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "fixedFactors",
			"type": "Variables"
		},
		{
			"name": "randomFactors",
			"type": "Variables"
		},
		{
			"name": "covariates",
			"type": "Variables"
		},
		{
			"name": "modelTerms",
			"template": [
				{
					"name": "components",
					"type": "Term"
				},
				{
					"name": "isNuisance",
					"type": "Boolean"
				}
			],
			"type": "Table"
		},
		{
			"name": "effects",
			"type": "Boolean"
		},
		{
			"name": "descriptives",
			"type": "Boolean"
		},
		{
			"name": "plotHorizontalAxis",
			"type": "Variable"
		},
		{
			"name": "plotSeparateLines",
			"type": "Variable"
		},
		{
			"name": "plotSeparatePlots",
			"type": "Variable"
		},
		{
			"name": "plotWidthDescriptivesPlotLegend",
			"type": "Integer",
			"default": 450
		},
		{
			"name": "plotHeightDescriptivesPlotLegend",
			"type": "Integer",
			"default": 300
		},
		{
			"name": "plotWidthDescriptivesPlotNoLegend",
			"type": "Integer",
			"default": 350
		},
		{
			"name": "plotHeightDescriptivesPlotNoLegend",
			"type": "Integer",
			"default": 300
		},
		{
			"name": "plotCredibleInterval",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.000,
			"name": "plotCredibleIntervalInterval",
			"type": "Number",
			"value": 0.95
		},
		{
			"name": "effectsStepwise",
			"type": "Boolean"
		},
		{
			"name": "posteriorEstimates",
			"type": "Boolean"
		},
		{
			"name": "posteriorDistributions",
			"type": "Boolean"
		},
		{
			"name": "bayesFactorType",
			"type": "List",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10"
		},
		{
			"name": "posteriorEstimatesCredibleIntervalInterval",
			"type": "Number",
			"format": "%",
			"value": 0.95,
			"min": 0.5,
			"max": 0.999
		},
		{
			"name": "posteriorEstimatesMCMCIterations",
			"type": "Number",
			"value": 10000,
			"min": 1,
			"max": 1000000
		},
		{
			"name": "sampleMode",
			"type": "List",
			"options": [ "auto", "manual" ]
		},
		{
			"name": "fixedSamplesNumber",
			"type": "Integer",
			"default": 10000,
			"min": 0
		},
		{
			"name": "priorFixedEffects",
			"type": "Number",
			"value": 0.5,
			"min": 0,
			"max": 2
		},
		{
			"name": "priorRandomEffects",
			"type": "Number",
			"value": 1,
			"min": 0,
			"max": 2
		},
		{
			"name": "priorCovariates",
			"type": "Number",
			"value": 0.354,
			"min": 0,
			"max": 2
		}
	]
}
