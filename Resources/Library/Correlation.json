{
	"name": "Correlation",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "variables",
			"type": "Variables"
		},
		{
			"default": true,
			"name": "pearson",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "kendallsTauB",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "spearman",
			"type": "Boolean"
		},
		{
			"name": "hypothesis",
			"options": [ "correlated", "correlatedPositively", "correlatedNegatively" ],
			"default": "correlated",
			"type": "List"
		},
		{
			"default": true,
			"name": "reportSignificance",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "flagSignificant",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "confidenceIntervals",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 1.0,
			"min": 0.0,
			"name": "confidenceIntervalsInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"default": false,
			"name": "meansAndStdDev",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "crossProducts",
			"type": "Boolean"
		},
		{
			"name": "missingValues",
			"options": [ "excludePairwise", "excludeListwise" ],
			"default": "excludePairwise",
			"type": "List"
		},
		{
			"default": false,
			"name": "plotCorrelationMatrix",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotDensities",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotStatistics",
			"type": "Boolean"
		},
		{
		  "name": "VovkSellkeMPR",
		  "type": "Boolean",
		  "default": false
		}
	]
}
